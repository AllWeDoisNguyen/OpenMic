<h1>Edit Review</h1>
<%= form_for([@comedian, @review]) do |f| %>
	<div class="field">
		<%= f.label :rating %><br>
		<%= f.number_field :rating %>
	</div>	
	<div class="field">
		<%= f.label :comment %><br>
		<%= f.text_field :comment %>
	</div>
	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>
